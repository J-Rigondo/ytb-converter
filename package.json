{
  "name": "ytb-converter",
  "version": "3.2.0",
  "main": "src/main.js",
  "author": "futuregoing",
  "license": "MIT",
  "description": "input youtube link, get audio",
  "keywords": [
    "electron",
    "youtube-mp3-converter",
    "electron-bulder",
    "ffmpeg"
  ],
  "scripts": {
    "start": "electron .",
    "deploy": "rimraf dist && electron-builder"
  },
  "build": {
    "productName": "YtbConverter",
    "appId": "com.futuregoing.ytbconverter",
    "asar": false,
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "devDependencies": {
    "electron": "^31.0.1",
    "electron-builder": "^24.13.3",
    "electron-reload": "^2.0.0-alpha.1",
    "rimraf": "^5.0.7"
  },
  "dependencies": {
    "electron-log": "^5.1.5",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "youtubei.js": "^15.1.1"
  }
}
